{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "extends": "airbnb",
  "rules": {
    "jsx-a11y/no-static-element-interactions": "off",
    "jsx-a11y/no-noninteractive-element-interactions": "warn",
    "object-curly-newline": "warn",
    "max-len": "warn",
  },
  "globals": {
    "window": true,
    "document": true,
    "localStorage": true,
  }
}
